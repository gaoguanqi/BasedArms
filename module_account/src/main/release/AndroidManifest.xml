    package="com.maple.module_account">

    <application>
        <activity android:name=".mvp.ui.activity.LoginActivity"></activity>
        <!-- 在组件集成宿主时, 由于宿主已经声明了 CommonSDK 的公有 ConfigModule, 所以在这里只需要声明业务组件自己的 ConfigModule -->
